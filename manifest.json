{
    "manifest_version": 3,
    "name": "Transfer Request",
    "version": "0.2.2",
    "description": "Inspect cookies and find access_token.",
    "permissions": ["cookies", "activeTab", "notifications", "tabs"],
    "host_permissions": ["<all_urls>"],
    "action": {
      "default_popup": "ui/popup.html",
      "default_icon": {
        "16": "assets/icon.png",
        "48": "assets/icon.png",
        "128": "assets/icon.png"
      }
    },
    "background": {
      "service_worker": "scripts/background.js",
      "type": "module"
    }
  }
  